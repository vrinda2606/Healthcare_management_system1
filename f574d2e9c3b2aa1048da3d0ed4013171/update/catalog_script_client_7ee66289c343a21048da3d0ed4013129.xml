        <name>Validate Appointment Date Client Script</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onSubmit() {
  var appointmentDate = g_form.getValue('appointment_date');
  var currentDate = new Date();

  var apptDate = new Date(appointmentDate);
  apptDate.setHours(0,0,0,0);
  currentDate.setHours(0,0,0,0);

  if (apptDate <= currentDate) {
    alert("Appointment date must be in the future.");
    return false;
  }
  return true;
}]]></script>
