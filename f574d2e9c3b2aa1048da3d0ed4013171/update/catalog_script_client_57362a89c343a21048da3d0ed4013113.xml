        <name>Load Doctors on Department Change Client</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
  if (isLoading || newValue == '') return;

  var ga = new GlideAjax('DoctorUtils');
  ga.addParam('sysparm_name', 'FetchDoctorByDepartment');
  ga.addParam('sysparm_department', newValue);

  ga.getXMLAnswer(function(response) {
    var doctorList = JSON.parse(response);

    g_form.clearOptions('doctor');
    g_form.addOption('doctor', '', '-- Select a Doctor --');

    doctorList.forEach(function(doc) {
      g_form.addOption('doctor', doc.sys_id, doc.name);
    });
  });
}]]></script>
